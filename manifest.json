{
    "manifest_version": 2,
    "name": "bantasy new tab",
    "version": "1.2.2",
    "description": "bantasy new tab",
    "permissions": [
        "storage",
        "tabs"
    ],
    "chrome_url_overrides": {
        "newtab": "index.html"
    },
    "icons": {
        "16": "logo.png",
        "32": "logo.png",
        "64": "logo.png",
        "128": "logo.png"
    },
    "browser_action": {
        "default_icon": {
            "16": "logo.png",
            "24": "logo.png",
            "32": "logo.png"
        },
        "default_popup": "popup.html"
    }
}